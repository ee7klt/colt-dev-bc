<% include ../partials/header %>




  <div class="container">
 <div >
   <form action='/campgrounds/new'>
   <button class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored">
   Create new campground
   </button>
   </form>
   </div>

    <div class="campgrounds">
      <% campgrounds.forEach(x => { %>

        <div class='mdl-card'>
            <a href='/campgrounds/<%= x.id %>' style='position:absolute; top:0; left:0;height:100%;width:100%'></a>
          <div class='mdl-card__title mdl-card--expand' style='background-image:url(<%=x.image%>); background-size:cover'>
            <h2 class='mdl-card__title-text'>
              <%= x.name %>
            </h2>
          </div>

        </div>
      <% }) %>
    </div>
<h4><%= currentUser %><h4>
  </div>
  <div  style=' background-image:url(images/camp1.jpg)'>

    </div>
    <script src="/script.js"></script>
  </body>
